// Realizar un Algoritmo en el cual el programa sumara 2 numeros cualquiera.
// Variables: a, b, resultado
// Salida: x

Funcion  expresion()
	Definir a,b,resultado como enteros; 
	a <- 10;
	Escribir "Digite un numero: ";
	leer b;
	
	resultado <- a+b;
	
	Escribir "El resultado es: ",resultado;
	
FinFuncion

Algoritmo Suma_de_numeros
	expresion()
FinAlgoritmo


