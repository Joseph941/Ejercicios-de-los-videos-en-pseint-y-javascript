// Realizar un Algoritmo en el cual el programa resuelva una operacion lógica. 
// Variables: a, b, c, resultado
// Salida: x

Funcion expresion() 
	Definir  a,b,c como enteros;
	Definir resultado Como Logico;
	
	a <- 10;
	b <- 15;
	c <- 10;
	
	resultado <- NO ((a>b) o (b>c));
	
	Escribir resultado;
FinFuncion

Algoritmo V_F_operacionLogica
	expresion()
FinAlgoritmo
	